{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TwitchRecentResponse",
  "type": "object",
  "required": ["ok", "limit", "count", "items"],
  "properties": {
    "ok": { "type": "boolean" },
    "limit": { "type": "integer" },
    "count": { "type": "integer" },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "event_type", "commit_ts", "payload"],
        "properties": {
          "id": { "type": "integer" },
          "event_type": { "type": "string" },
          "commit_ts": { "type": "string" },
          "user_id": { "type": ["string", "null"] },
          "payload": { "type": "object" },
          "created_at_utc": { "type": ["string", "null"] }
        }
      }
    }
  }
}
